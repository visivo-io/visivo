        "hoverinfo": {
          "oneOf": [
            {
              "description": "Determines which trace information appear when hovering nodes. If `none` or `skip` are set, no information is displayed upon hovering. But, if `none` is set, click and hover events are still fired.",
              "enum": [
                "all",
