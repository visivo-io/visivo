{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://0.0.0.0:8000",
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.33.1-dev16.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/system": "^6.5.0",
    "@mui/x-date-pickers": "^7.29.4",
    "@mui/x-tree-view": "^8.23.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "copy-to-clipboard": "^3.3.3",
    "dagre": "^0.8.5",
    "export-to-csv": "^1.4.0",
    "flowbite-react": "^0.12.15",
    "html2canvas-pro": "^1.6.3",
    "jest-canvas-mock": "^2.5.2",
    "lodash": "^4.17.21",
    "material-react-table": "^3.2.1",
    "md5": "^2.3.0",
    "monaco-editor": "^0.55.1",
    "plotly.js": "^3.3.1",
    "react": "^18.3.1",
    "react-cool-dimensions": "^3.0.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.1.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.30.2",
    "react-select": "^5.10.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^4.1.18",
    "vite": "^6.4.1",
    "web-vitals": "^3.5.2",
    "zustand": "^5.0.9"
  },
  "scripts": {
    "start": "yarn run start:local",
    "start:local": "cp src/LocalProviders.jsx src/Providers.jsx && vite",
    "start:dist": "cp src/DistProviders.jsx src/Providers.jsx && vite",
    "test": "jest",
    "build": "vite build",
    "deploy": "yarn run deploy:local && yarn run deploy:dist",
    "deploy:local": "cp src/LocalProviders.jsx src/Providers.jsx && yarn run build && rm -rf ../visivo/viewers/local && cp -Rv build/ ../visivo/viewers/local/",
    "deploy:dist": "cp src/DistProviders.jsx src/Providers.jsx && yarn run build && rm -rf ../visivo/viewers/dist && cp -Rv build/ ../visivo/viewers/dist/",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\?url$": "<rootDir>/__mocks__/fileMock.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!export-to-csv)/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@types/react": "^18.3.23",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "history": "^5.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "react-select-event": "^5.5.1",
    "react-test-renderer": "^18.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "unist-util-visit-parents": "^5.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "whatwg-fetch": "^3.6.20"
  },
  "resolutions": {
    "unist-util-visit-parents": "^5.0.0"
  }
}
