    "zustand": "^5.0.3"
  },
  "scripts": {
    "start": "yarn run start:local",
    "start:local": "cp src/LocalProviders.jsx src/Providers.jsx && vite",
    "start:dist": "cp src/DistProviders.jsx src/Providers.jsx && vite",
    "start:embed": "cd ../test-projects/integration && python3 -m http.server 3000",
    "test": "jest",
    "build": "vite build",
    "build:embed": "yarn run build:embed:lib && yarn run build:embed:global",
    "build:embed:lib": "vite build --config vite.embed.config.mjs --mode library",
    "build:embed:global": "vite build --config vite.embed.config.mjs --mode global",
    "deploy": "yarn run deploy:local && yarn run deploy:dist",
    "deploy:local": "cp src/LocalProviders.jsx src/Providers.jsx && yarn run build && rm -rf ../visivo/viewers/local && cp -Rv build/ ../visivo/viewers/local/",
    "deploy:dist": "cp src/DistProviders.jsx src/Providers.jsx && yarn run build && rm -rf ../visivo/viewers/dist && cp -Rv build/ ../visivo/viewers/dist/",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!export-to-csv)/",
      "<rootDir>/node_modules/(?!d3)/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.23.0",
    "@testing-library/dom": "^10.4.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "history": "^5.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "react-select-event": "^5.5.1",
    "react-test-renderer": "^19.0.0",
    "unist-util-visit-parents": "^5.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "whatwg-fetch": "^3.6.20"
  },
  "resolutions": {
    "unist-util-visit-parents": "^5.0.0"
  }
}
